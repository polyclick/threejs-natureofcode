{
  "name": "polyclick-starter",
  "version": "0.2.0",
  "description": "Very opinionated starter (es6+, babel, gulp, jspm, eslint, sass)",
  "main": "js/app.js",
  "homepage": "https://github.com/polyclick/polyclick-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polyclick/polyclick-starter.git"
  },
  "bugs": "https://github.com/polyclick/polyclick-starter/issues",
  "author": {
    "name": "Bart Claessens",
    "email": "hello@polyclick.io",
    "url": "https://polyclick.io"
  },
  "readmeFilename": "README.md",
  "license": "Unlicense",
  "scripts": {
    "build": "gulp build",
    "start": "gulp",
    "postinstall": "jspm install"
  },
  "keywords": [
    "es6",
    "jspm",
    "gulp",
    "boilerplate",
    "babel",
    "systemjs",
    "threejs",
    "stackgl",
    "pixi.js"
  ],
  "directories": {
    "baseURL": "src",
    "packages": "src/lib"
  },
  "devDependencies": {
    "eslint": "^4.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-eslint": "^4.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^3.2.0",
    "gulp-jspm": "^0.5.13",
    "gulp-minify-html": "^1.0.6",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-sass-lint": "^1.3.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-util": "^3.0.8",
    "gulp-webserver": "^0.9.1",
    "imagemin-pngquant": "^5.0.0",
    "jspm": "^0.17.0-beta.41",
    "require-dir": "^0.3.1",
    "run-sequence": "^2.1.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "src",
      "doc": "docs",
      "packages": "src/lib"
    },
    "dependencies": {
      "dat-gui": "npm:dat-gui@^0.5.0",
      "gsap": "npm:gsap@^1.20.3",
      "three": "github:mrdoob/three.js@master"
    },
    "devDependencies": {
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.21",
      "process": "npm:jspm-nodelibs-process@^0.2.0"
    },
    "overrides": {
      "github:mrdoob/three.js@master": {
        "format": "global"
      },
      "npm:babel-runtime@5.8.35": {
        "main": false,
        "dependencies": {},
        "optionalDependencies": {
          "core-js": "^1.2.0"
        }
      }
    }
  },
  "dependencies": {}
}
